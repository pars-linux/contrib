+#: src/dbf.c:47
+#, c-format
+msgid "Copyright 2002-2004 Bjoern Berg"
+msgstr "Copyright 2002-2004 Bjoern Berg"
+
+#: src/dbf.c:86
+#, c-format
+msgid "Cannot close output file '%s'."
+msgstr "Çıktı dosyası kapatılamadı."
+
+#: src/dbf.c:92
+#, c-format
+msgid "Output file '%s' was closed successfully."
+msgstr "Çıktı dosyası başarıyla kapatıldı: \"%s\""
+
+#: src/dbf.c:190
+#, c-format
+msgid "Number of records to output must be greater zero."
+msgstr "Çıktı kayıtlarının sayısı sıfırdan büyük olmak zorundadır."
+
+#: src/dbf.c:251
+#, c-format
+msgid "Invalid debug level ``%s''. Must be from 0 to 9"
+msgstr "Geçersiz ayıklama seviyesi \"%s\". 0 ile 9 arasında olmak zorundadır."
+
+#: src/dbf.c:420
+#, c-format
+msgid "Usage: %s [options] dbf-file"
+msgstr "Kullanım: %s [seçenekler] dbf-dosyası"
+
+#: src/dbf.c:422
+#, c-format
+msgid "Output the contents of a dBASE table file (.dbf)."
+msgstr "dBASE tablo dosyası (.dbf) içeriğinin çıktısı."
+
+#: src/dbf.c:424
+#, c-format
+msgid "Available options:"
+msgstr "Uygun seçenekler:"
+
+#: src/dbf.c:434
+#, c-format
+msgid "The options --sql, --csv, --view-info, --version are mutually exclusive."
+msgstr "Seçenekler: --sql, --csv, --view-info, --version "
+
+#: src/dbf.c:436
+#, c-format
+msgid "The last option specified takes precedence."
+msgstr "Son belirlenmi? seçenek önceliklidir."
+
+#: src/dbf.c:438
+#, c-format
+msgid "A single dash (``-'') as a filename specifies stdin or stdout"
+msgstr "Tekil dosya adı stdin veya stdout olarak belirtilir."
+
+#: src/dbf.c:489
+#, c-format
+msgid ""
+"No input file specified. Please make sure that the last argument is a valid "
+"dBASE file."
+msgstr "Giriş dosyası belirlenmedi.Lütfen son değişkenin geçerli bir dBASE dosyası olduğundan emin olun."
+
+#: src/dbf.c:496
+#, c-format
+msgid "Could not open dBASE file '%s'."
+msgstr "dBASE dosyası '%s' açılamadı."
+
+#: src/dbf.c:511
+#, c-format
+msgid "Unrecognized option ``%s''. Try ``--help'' for a list of options."
+msgstr "Geçersiz seçenek: \"%s\" Seçeneklerin listesi için --help 'i deneyiniz."
+
+#: src/dbf.c:519
+#, c-format
+msgid ""
+"Output file name was already specified as ``%s''. Try the --help for a list "
+"of options."
+msgstr "Çıktı dosyasının adı zaten ''%s\" olarak belirtildi. Seçeneklerin listesi için --help deneyiniz."
+
+#: src/dbf.c:528
+#, c-format
+msgid ""
+"Input file name is equal to output file name. Please choose a different "
+"output file name."
+msgstr "Giriş ve çıktı dosyalarının isimleri birbiriyle aynı. Lütfen farklı bir çıktı dosyası ismi belirleyin."
+
+#: src/dbf.c:569
+#, c-format
+msgid "Creating the table name from the export file name results in a NULL string."
+msgstr "Tablo ismi, bo? bir dizinin dosya ismi sonuularından kuruluyor."
+
+#: src/dbf.c:578
+#, c-format
+msgid "SQL mode requires a tablename to be set, if the output goes to stdout."
+msgstr "Çıktı stdout olduğunda, SQL modu, tablo isminin ayarlanmasını gerektiriyor."
+
+#: src/dbf.c:582
+#, c-format
+msgid "Tablename is '%s'"
+msgstr "Tablo adı '%s'"
+
+#: src/dbf.c:601
+#, c-format
+msgid "Export from '%s' to '%s'"
+msgstr "'%s' dosyasından '%s' dosyasına çıkart"
+
+#: src/dbf.c:617
+#, c-format
+msgid "Will only output %d rows, because there are no more rows in the dBASE file."
+msgstr "Sadece %d dizisi çıkarılacak, çünkü dBASE dosyasında başka dizi yok."
+
+#: src/dbf.c:622
+#, c-format
+msgid "Output records from row %d to %d."
+msgstr "Çıktı kayıtları %d dizisinden %d dizisine."
+
+#: src/dbf.c:628
+#, c-format
+msgid "Row %i."
+msgstr "%i dizisi."
+
+#: src/dbf.c:647
+#, c-format
+msgid "The row %i is set to 'deleted'."
+msgstr "'%i' dizisi 'silindi' olarak işaretlendi."
+
+#: src/sql.c:95
+#, c-format
+msgid "Invalid trim mode ``%s''. Expecting ``r'', ``l'', or ``b'' for both."
+msgstr "Geçersiz düzeltme modu: %s. İkisi için yalnızca ``r'', ``l'', ya da ``b'' "
+
+#.
+#. * M stands for memo fields which are currently not
+#. * supported by dbf.
+#. * - berg, 2003-09-08
+#.
+#: src/sql.c:158 src/sql.c:188
+#, c-format
+msgid "Invalid mode. Cannot convert this dBASE file. Memo fields are not supported."
+msgstr "Geçersiz mod. dBASE dosyası dönüştürülemiyor. Hafıza alanları desteklenmiyor."
+
+#: src/csv.c:33
+#, c-format
+msgid "Separator / Escape char ``%s'' is too long -- must be a single character."
+msgstr "Ayırıcı / çıktı karakteri %s çok uzun -- tekil bir karakter olmak zorunda."
+
+#: src/statistic.c:44
+#, c-format
+msgid "Unknown (code 0x%.2X)"
+msgstr "Bilinmeyen (kod 0x%.2x)"
+
+#: src/statistic.c:59
+#, c-format
+msgid "File statistics:"
+msgstr "Dosya istatistiği:"
+
+#: src/statistic.c:61
+#, c-format
+msgid "dBase version.........:"
+msgstr "dBase Versiyonu......:"
+
+#: src/statistic.c:64
+#, c-format
+msgid "Date of last update...:"
+msgstr "Son güncellenme tarihi..:"
+
+#: src/statistic.c:66
+#, c-format
+msgid "Number of records.....:"
+msgstr "Kayıt numarası.......:"
+
+#: src/statistic.c:68
+#, c-format
+msgid "Length of header......:"
+msgstr "Başlık uzunluğu......:"
+
+#: src/statistic.c:70
+#, c-format
+msgid "Record length.........:"
+msgstr "Kayıt uzunluğu.......:"
+
+#: src/statistic.c:72
+#, c-format
+msgid "Columns in file.......:"
+msgstr "Dosyadaki sütunlar...:"
+
+#: src/statistic.c:89
+#, c-format
+msgid "field name"
+msgstr "Alan adı"
+
+#: src/statistic.c:91
+#, c-format
+msgid "type"
+msgstr "Tip"
+
+#: src/statistic.c:93
+#, c-format
+msgid "field address"
+msgstr "Alan adresi"
+
+#: src/statistic.c:95
+#, c-format
+msgid "length"
+msgstr "Uzunluk"
+
+#: src/statistic.c:97
+#, c-format
+msgid "field dec."
+msgstr ""
