    <Package>
       <Name>xfce4-panel</Name>
       <RuntimeDependencies>
            <Dependency versionFrom="4.4.3">libxfcegui4</Dependency>
            <Dependency versionFrom="4.4.3">xfce-mcs-manager</Dependency>
            <Dependency versionFrom="0.3.4">exo</Dependency>
            <Dependency>startup-notification</Dependency>
       </RuntimeDependencies>
       <Files>
           <Path fileType="executable">/usr/bin</Path>
           <Path fileType="config">/etc</Path>
           <Path fileType="header">/usr/include</Path>
           <Path fileType="library">/usr/lib</Path>
           <Path fileType="localedata">/usr/share/locale</Path>
           <Path fileType="doc">/usr/share/doc</Path>
           <Path fileType="doc">/usr/share/xfce4/doc</Path>
           <Path fileType="data">/usr/share</Path>
       </Files>
       <AdditionalFiles>
           <!-- panel settings -->
           <AdditionalFile owner="root" permission="0644" target="/etc/xdg/xfce4/panel/panels.xml">panels.xml</AdditionalFile>
           <AdditionalFile owner="root" permission="0644" target="/etc/xdg/xfce4/panel/xfce4-menu-2.rc">xfce4-menu-2.rc</AdditionalFile>
