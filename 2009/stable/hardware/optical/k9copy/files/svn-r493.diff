-
-  lbnum = partition->Start + ICB.Location;
-  tmpmap.lbn = lbnum;
-  if(GetUDFCache(device, MapCache, lbnum, &tmpmap)) {
-    *FileType = tmpmap.filetype;
-    *File = tmpmap.file;
-    return 1;
-  }
-
-  LogBlock = dvdalign_lbmalloc(device, 1);
-  if(!LogBlock) {
-    return 0;
-  }
-    
-  do {
-    if( DVDReadLBUDF( device, lbnum++, 1, LogBlock, 0 ) <= 0 ) {
