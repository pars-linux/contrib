-static int UDFScanDir( dvd_reader_t *device, struct AD Dir, char *FileName,
-                       struct Partition *partition, struct AD *FileICB,
-                       int cache_file_info) 
-{
-  char filename[ MAX_UDF_FILE_NAME_LEN ];
-  uint8_t *directory;
-  uint32_t lbnum;
-  uint16_t TagID;
-  uint8_t filechar;
-  unsigned int p;
