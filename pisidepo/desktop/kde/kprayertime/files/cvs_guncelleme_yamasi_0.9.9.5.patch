+++ kprayertime/src/frmtestimpl.cpp	2006-05-31 15:31:47.000000000 +0300
@@ -0,0 +1,161 @@
+//
+// C++ Implementation: frmtestimpl
+//
+// Description:
+//
+//
+// Author: Abdulhaq Lynch <abdulhaq@users.sourceforge.net>, (C) 2006
+//
+// Copyright: See COPYING file that comes with this distribution
+//
+//
+#include "frmtestimpl.h"
+
+class GUIUpdateEvent : public QCustomEvent
+{
+  public:
+    GUIUpdateEvent(double lat, double longi, double tests, double fails);
+    double latitude;
+    double longitude;
+    double numTests, numFails;
+};
+
+GUIUpdateEvent::GUIUpdateEvent(double lat, double longi, double tests, double fails) : QCustomEvent( 65432 ){
+  this->latitude = lat;
+  this->longitude = longi;
+  this->numTests = tests;
+  numFails = fails;
+}
+
+class TestRunner : public QThread
+{
+
+public:
+  TestRunner(TestResults*, QMutex*, QWidget*);
+  virtual void run();
+  void updateGUI(double, double,double, double);
+  TestResults* results;
+  QMutex* mutex;
+  QWidget* testGUI;
+  /**
+  * pointers to salat times in array in caclTime
+   **/
+  Prayer *ptime;
+};
+
+void TestRunner::updateGUI(double la, double lo,double tests, double fails)
+{
+  GUIUpdateEvent* ev = new GUIUpdateEvent(la,lo,tests,fails);
+  QApplication::postEvent(testGUI,ev);
+}
+
+TestRunner::TestRunner(TestResults* res, QMutex* m, QWidget* GUI)
+{
+  results = res;
+  mutex = m;
+  testGUI = GUI;
+}
+
+void TestRunner::run()
+{
+  calcTime* calc = new calcTime();
+
+  calc->setMethod(1) ;
+
+  calc->setErrorCorrection(2,2);
+  calc->setZawaal(2);
+
+  mutex->lock();
+  results->textResults = "Start Test\n";
+  results->numTests = 0;
+  results->numFails = 0;
+  mutex->unlock();
+
+  for (double lat = -69.9; lat<70; lat += .1)
+  {
+    for (double longi = -179.9; longi<180; longi += .1)
+    {
+      calc->setPosition(lat, longi, 0);
+
+      int timeZone = int(longi/15);
+      calc->setTimeZone(timeZone, TRUE, TZR_EUROPE);
+      ptime = calc->makeSchedule();
+
+      for (int pIndex = 0; pIndex < 6; pIndex++)
+      {
+        // test that hours and minutes are with 0-23 and 0-59
+        if ( not (ptime[pIndex].hour >= 0 && ptime[pIndex].hour <= 23 && ptime[pIndex].minute >= 0 && ptime[pIndex].minute <=59 ))
+        {
+          QString sTZ = QString::number(timeZone);
+          QString sLat = QString::number(lat);
+          QString sLong = QString::number(longi);
+          QString sHour = QString::number(ptime[pIndex].hour);
+          QString sMinute = QString::number(ptime[pIndex].minute);
+          QString sIndex = QString::number(pIndex);
+          QString sTime = sLat + "-" + sLong + " (" + sTZ + ") " + sIndex + " " + sHour + ":" + sMinute + "\n";
+          mutex->lock();
+          results->textResults += sTime;
+          results->numFails++;
+          mutex->unlock();
+        }
+        mutex->lock();
+        results->numTests++;
+        mutex->unlock();
+        updateGUI(lat,longi,results->numTests,results->numFails);
+      }
+      msleep(20);
+
+    }
+
+  }
+}
+
+frmTestImpl::frmTestImpl(QWidget* parent,  const char* name, bool modal, WFlags fl )
+    : frmTest(parent, name, modal, fl)
+{
+  mutex = new QMutex();
+  results = new TestResults();
+}
+
+
+frmTestImpl::~frmTestImpl()
+{
+  delete mutex;
+  delete results;
+  //delete thread;
+}
+
+void frmTestImpl::doTestCalculation()
+{
+
+
+  QThread* thread = new TestRunner(results, mutex, (QWidget*) this);
+  thread->start();
+
+}
+
+void frmTestImpl::customEvent( QCustomEvent * e )
+{
+  if ( e->type() == 65432 )
+  {
+    GUIUpdateEvent* ge = (GUIUpdateEvent *) e;
+    mutex->lock();
+    leTestOutput->setText(results->textResults);
+    QString sLat = QString::number(ge->latitude);
+    QString sLong = QString::number(ge->longitude);
+    QString sTests = QString::number(ge->numTests);
+    QString sFails = QString::number(ge->numFails);
+    leLat->setText(sLat);
+    leLong->setText(sLong);
+    leNumTests->setText(sTests);
+    leNumFails->setText(sFails);
+    mutex->unlock();
+  }
+}
+
+void frmTestImpl::doTestAdhaan()
+{
+  leTestOutput->setText("Test adhaan");
+}
+
+
