+
+enum exmethods  { NONE_EX,
+                  LAT_ALL,
+                  LAT_ALWAYS,
+                  LAT_INVALID,
+                  GOOD_ALL,
+                  GOOD_INVALID,
+                  SEVEN_NIGHT_ALWAYS,
+                  SEVEN_NIGHT_INVALID,
+                  SEVEN_DAY_ALWAYS,
+                  SEVEN_DAY_INVALID,
+                  HALF_ALWAYS,
+                  HALF_INVALID,
+                  MIN_ALWAYS,
+                  MIN_INVALID,
+                  GOOD_DIF };
+
+enum methods    { NONE,
+                  EGYPT_SURVEY,
+                  KARACHI_SHAF,
+                  KARACHI_HANAF,
+                  NORTH_AMERICA,
+                  MUSLIM_LEAGUE,
+                  UMM_ALQURRA,
+                  FIXED_ISHAA };
+
+enum salatType  { FAJR,
