+msgid "Klineak  Keyboard  Definition Tool"
+msgstr "Klineak Klavye Tanımlama Aracı"
+
+#: klineakdefui.cpp:168
+msgid "Keyboard"
+msgstr "Klavye"
+
+#: klineakdefui.cpp:172
+msgid "Key Name:"
+msgstr "Tuş adı"
+
+#: klineakdefui.cpp:173 klineakdefui.cpp:177
+msgid "Enter the name of a key you wish to define."
+msgstr "Tanımlamak istediğiniz tuşun ismini girin."
+
+#: klineakdefui.cpp:181
+msgid "Key Code:"
+msgstr "Tuş kodu:"
+
+#: klineakdefui.cpp:182 klineakdefui.cpp:187
